# Appwrite Configuration (Database)
# Get these from: https://cloud.appwrite.io/
VITE_APPWRITE_ENDPOINT=https://cloud.appwrite.io/v1
VITE_APPWRITE_PROJECT_ID=your-project-id
VITE_APPWRITE_DATABASE_ID=your-database-id

# Stack Auth Configuration (Authentication)
# Get these from: https://app.stack-auth.com/
VITE_STACK_PROJECT_ID=your-stack-project-id
VITE_STACK_PUBLISHABLE_CLIENT_KEY=your-publishable-client-key

# Gemini AI Configuration (Fallback)
# Get your API key from: https://aistudio.google.com/app/apikey
VITE_GEMINI_API_KEY=your-gemini-api-key-here
VITE_GEMINI_MODEL=gemini-2.5-flash

# OpenRouter Configuration (Primary AI Provider)
# Get your API key from: https://openrouter.ai/
VITE_OPENROUTER_API_KEY=your-openrouter-api-key-here
VITE_OPENROUTER_MODEL=mistralai/devstral-2512:free
VITE_USE_AI_PROXY=false

# LangSearch Configuration (Primary Search)
# Get your API key from: https://langsearch.io/
VITE_LANGSEARCH_API_KEY=your-langsearch-api-key-here

# Tavily Configuration (Backup Search)
# Get your API key from: https://tavily.com/
VITE_TAVILY_API_KEY=your-tavily-api-key-here

# Backend API endpoint for AI services (e.g., https://your-backend.com/api/ai)
VITE_AI_PROXY_URL=https://your-backend.com/api/ai
